all:
	$(MAKE) -f Makefile.native all
	$(MAKE) -f Makefile.pic30 all
	$(MAKE) -f Makefile.arm all

clean:
	$(MAKE) -f Makefile.native clean
	$(MAKE) -f Makefile.pic30 clean
	$(MAKE) -f Makefile.arm clean

distclean:
	$(MAKE) -f Makefile.native distclean
	$(MAKE) -f Makefile.pic30 distclean
	$(MAKE) -f Makefile.arm distclean
